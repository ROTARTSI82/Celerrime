    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "GameTest gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/GameTest",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },

        {
            "name": "png2rim gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/misc/a.out",
            "args": ["./quote.png", "./out.rim"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/misc",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}